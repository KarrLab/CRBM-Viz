{
  "projectType": "library",
  "root": "libs/view/visualizations",
  "sourceRoot": "libs/view/visualizations/src",
  "prefix": "biosimulations",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/view/visualizations/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/view/visualizations/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "generators": {
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "tags": ["scope:shared", "type:ui", "platform:web"],
  "implicitDependencies": ["shared-styles"]
}
